{
  "parserOptions": {
    "sourceType": "module",
  },
  "globals": {},
  "env": {
    "node": true,
    "mocha": true,
  },
  "rules": {
    "max-len": ["error", 100, { "ignoreComments": true, "ignoreTrailingComments": true }],
    "indent": [2, 2],
    "quotes": [2, "single"],
    "linebreak-style": [2,"unix"],
    "semi": [2, "always"],
    "no-console": "error"
  },
  "extends": ["google"]
}
